<system>
	
    
	<modules>
		
        
		<module name="x86.rtos-rigel">
			
            
			<prefix>rtos</prefix>
			
            
			<signalset_size>8</signalset_size>
			
            
			<api_asserts>true</api_asserts>
			
            
			<internal_asserts>true</internal_asserts>
			
            
			<fatal_error>fatal</fatal_error>
			
            
			<tasks>
				
                
				<task>
					
                    
					<name>one</name>
					
                    
					<function>task_one</function>
					
                    
					<stack_size>8192</stack_size>
					
                    
					<start>true</start>
					
                
				</task>
				
                
				<task>
					
                    
					<name>two</name>
					
                    
					<function>task_two</function>
					
                    
					<stack_size>8192</stack_size>
					
                
				</task>
				
                
				<task>
					
                    
					<name>three</name>
					
                    
					<function>task_three</function>
					
                    
					<stack_size>8192</stack_size>
					
                
				</task>
				
                
				<task>
					
                    
					<name>four</name>
					
                    
					<function>task_four</function>
					
                    
					<stack_size>8192</stack_size>
					
                
				</task>
				
                
				<task>
					
                    
					<name>five</name>
					
                    
					<function>task_five</function>
					
                    
					<stack_size>8192</stack_size>
					
                
				</task>
				
            
			</tasks>
			
            
			<signal_labels>
				
                
				<signal_label>
					
                    
					<name>one</name>
					
                    
					<global>true</global>
					
                
				</signal_label>
				
                
				<signal_label>
					
                    
					<name>two</name>
					
                    
					<global>true</global>
					
                
				</signal_label>
				
                
				<signal_label>
					
                    
					<name>three</name>
					
                    
					<global>true</global>
					
                
				</signal_label>
				
                
				<signal_label>
					
                    
					<name>four</name>
					
                    
					<global>true</global>
					
                
				</signal_label>
				
                
				<signal_label>
					
                    
					<name>five</name>
					
                    
					<global>true</global>
					
                
				</signal_label>
				
            
			</signal_labels>
			
            
			<timers>
				
                
				<timer>
					
                    
					<name>one</name>
					
                    
					<enabled>false</enabled>
					
                
				</timer>
				
                
				<timer>
					
                    
					<name>two</name>
					
                    
					<enabled>false</enabled>
					
                
				</timer>
				
            
			</timers>
			
            
			<interrupt_events>
				
                
				<interrupt_event>
					
                    
					<name>one</name>
					
                    
					<task>one</task>
					
                    
					<sig_set>one</sig_set>
					
                
				</interrupt_event>
				
                
				<interrupt_event>
					
                    
					<name>two</name>
					
                    
					<task>two</task>
					
                    
					<sig_set>two</sig_set>
					
                
				</interrupt_event>
				
                
				<interrupt_event>
					
                    
					<name>three</name>
					
                    
					<task>three</task>
					
                    
					<sig_set>three</sig_set>
					
                
				</interrupt_event>
				
                
				<interrupt_event>
					
                    
					<name>four</name>
					
                    
					<task>four</task>
					
                    
					<sig_set>four</sig_set>
					
                
				</interrupt_event>
				
                
				<interrupt_event>
					
                    
					<name>five</name>
					
                    
					<task>five</task>
					
                    
					<sig_set>five</sig_set>
					
                
