				</mutex>
				
                
				<mutex>
					
                    
					<name>three</name>
					
                
				</mutex>
				
                
				<mutex>
					
                    
					<name>four</name>
					
                
				</mutex>
				
                
				<mutex>
					
                    
					<name>five</name>
					
                
				</mutex>
				
            
			</mutexes>
			
            
			<mutex>
				
                
				<stats>false</stats>
				
            
			</mutex>
			
            
			<message_queues>
				
                
				<message_queue>
					
                    
					<name>one</name>
					
                    
					<message_size>100</message_size>
					
                    
					<queue_length>10</queue_length>
					
                
				</message_queue>
				
            
